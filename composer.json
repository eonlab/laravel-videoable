{
    "name": "eonlab/laravel-videoable",
    "type": "library",
    "description": "Attach videos to your models",
    "keywords": [
        "laravel",
        "laravel-videoable",
        "video",
        "youtube",
        "vimeo"
    ],
    "homepage": "https://github.com/nwidart/laravel-videoable",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Widart",
            "email": "n.widart@gmail.com",
            "homepage": "https://nicolaswidart.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/framework": "6.15.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "~8.0",
        "squizlabs/php_codesniffer": "^3.6",
        "orchestra/testbench": "4.2.*",
        "friendsofphp/php-cs-fixer": "^2.20"
    },
    "autoload": {
        "psr-4": {
            "Eonlab\\LaravelVideoable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eonlab\\LaravelVideoable\\tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "fix-style": "vendor/bin/php-cs-fixer fix . --config-file='.php_cs' --verbose"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "laravel": {
            "providers": [
                "Eonlab\\LaravelVideoable\\LaravelVideoableServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
